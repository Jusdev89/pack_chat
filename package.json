  },
  "dependencies": {
    "babel-cli": "~6.23.0",
    "babel-preset-es2015": "~6.22.0",
    "babel-preset-latest": "~6.22.0",
    "body-parser": "~1.16.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.0",
    "express": "~4.14.1",
    "knex": "~0.12.7",
    "morgan": "~1.7.0",
    "pg": "~6.1.2",
    "pug": "~2.0.0-beta10",
    "request": "~2.80.0",
    "request-promise": "~4.1.1",
    "serve-favicon": "~2.3.2"
  },
